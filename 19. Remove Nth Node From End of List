/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        ListNode start = head;
        ListNode end = head;
        for(int i = 0; i < n; i++){
            if(start == null){
                return null;
            }
            start = start.next;
        }
        if(start == null){
            return head.next;
        }
        while(start.next != null){
            end = end.next;
            start = start.next;
        }
        end.next = end.next.next;
        return head;
    }
}
